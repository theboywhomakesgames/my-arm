:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x55c5404852f0 .scope module, "clocktest" "clocktest" 2 3;
 .timescale 0 0;
v0x55c540495640_0 .net "clk1", 0 0, v0x55c54044c270_0;  1 drivers
v0x55c540495710_0 .net "clk2", 0 0, v0x55c540495460_0;  1 drivers
E_0x55c54044b290 .event anyedge, v0x55c54044c270_0, v0x55c540495460_0;
S_0x55c5404854a0 .scope module, "clk" "clock" 2 5, 3 3 0, S_0x55c5404852f0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "c1";
    .port_info 1 /OUTPUT 1 "c2";
v0x55c54044c270_0 .var "c1", 0 0;
v0x55c540495460_0 .var "c2", 0 0;
v0x55c540495520_0 .var/i "phase", 31 0;
    .scope S_0x55c5404854a0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c540495520_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x55c5404854a0;
T_1 ;
    %delay 40, 0;
    %load/vec4 v0x55c540495520_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c54044c270_0, 0, 1;
T_1.0 ;
    %load/vec4 v0x55c540495520_0;
    %cmpi/e 8, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c54044c270_0, 0, 1;
T_1.2 ;
    %load/vec4 v0x55c540495520_0;
    %cmpi/e 10, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c540495460_0, 0, 1;
T_1.4 ;
    %load/vec4 v0x55c540495520_0;
    %cmpi/e 18, 0, 32;
    %jmp/0xz  T_1.6, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c540495460_0, 0, 1;
T_1.6 ;
    %load/vec4 v0x55c540495520_0;
    %cmpi/e 20, 0, 32;
    %jmp/0xz  T_1.8, 4;
    %pushi/vec4 4294967295, 0, 32;
    %store/vec4 v0x55c540495520_0, 0, 32;
T_1.8 ;
    %load/vec4 v0x55c540495520_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55c540495520_0, 0, 32;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c5404852f0;
T_2 ;
    %wait E_0x55c54044b290;
    %vpi_call 2 8 "$display", "c1: %b -- c2: %b", v0x55c540495640_0, v0x55c540495710_0 {0 0 0};
    %jmp T_2;
    .thread T_2, $push;
# The file index is used to find the file name in the following table.
